.brand_detail .logo_area {
    position: absolute;
    top: -70rpx;
    left: 50%;
    transform: translateX(-50%);
    height: 140rpx;
    width: 140rpx;
    border-radius: 24rpx;
    background-color: #fff;
}

.brand_detail .logo_area image.star {
    position: absolute;
    bottom: -20rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 50rpx;
    height: 48rpx;
}

.brand_detail .logo_area image:first-child {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: 140rpx;
    border-radius: 24rpx;
}

.brand_detail .brand_name {
    padding-top: 100rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 700;
    letter-spacing: 1px;
    color: #112a51;
}

.brand_detail .one_punch_line {
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    font-size: 28rpx;
    letter-spacing: 1px;
    color: #112a51;
}

.brand_detail .leaf_type {
    margin-bottom: 10rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 26rpx;
    letter-spacing: 1px;
    color: rgb(138, 140, 150);
}

.brand_detail .hight_light {
    display: flex;
    margin-bottom: 10rpx;
    padding: 20rpx 0;
    font-size: 32rpx;
}

.brand_detail .hight_light .hight_light_item {
    flex: 1;
    text-align: center;
}

.brand_detail .hight_light .hight_light_item image {
    width: 44rpx;
    height: 40rpx;
}

.brand_detail .hight_light .hight_light_item .diff {
    height: 42rpx;
    line-height: 42rpx;
    text-align: center;
    font-size: 30rpx;
    font-weight: 700;
    opacity: 0.6;
    color: #112a51;
}

.brand_detail .story {
    line-height: 36rpx;
    font-size: 26rpx;
    opacity: 0.5;
    color: #112a51;
    letter-spacing: 1px;
}

.brand_descript {
    margin-bottom: 20rpx;
    position: relative;
}

.brand_descript .child {
    position: absolute;
    top: 0;
    right: 40rpx;
    width: 60rpx;
    height: 68rpx;
}

.descript_title {
    margin: 20rpx 0;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 44rpx;
    text-align: center;
    letter-spacing: 1px;
    color: #34384a;
}

.brand_descript .title {
    font-size: 0;
    margin-bottom: 10rpx;
}

.brand_descript .title text {
    display: inline-block;
    height: 42rpx;
    line-height: 42rpx;
    vertical-align: middle;
}

.brand_descript .title text:last-child {
    font-size: 30rpx;
    font-weight: 700;
    letter-spacing: 1px;
    color: #112a51;
}

.brand_descript .title .point {
    width: 10rpx;
    height: 10rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.point_green {
    background: #59d5c7;
}

.point_yellow {
    background: #ffdc6c;
}

.brand_descript .descript_content {
    line-height: 42rpx;
    font-size: 30rpx;
    color: #34384a;
    letter-spacing: 1px;
}

.bottom_blank {
    height: 20rpx;
}
