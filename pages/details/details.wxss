/* pages/details/details.wxss */

page {
    height: 100%;
}

.content_has_padding {
    background-image: url(https://dongdakid.com/assets/images/bg_detail@2x.png);
}

.top_blank_nav {
    height: 44px;
    padding-top: 130rpx;
}

.top_blank_android {
    height: 48px;
    padding-top: 138rpx;
}

.top_blank_iosx {
    padding-top: 178rpx;
}

.brand_info {
    margin-top: 570rpx;
    padding: 40rpx;
    border-radius: 24rpx;
    background-color: #fff;
}

.brand_detail {
    position: relative;
}

.got_egg {
    position: fixed;
    /* z-index: 0; */
    left: 50%;
    transform: translateX(-50%);
    width: 590rpx;
    height: 590rpx;
    background: #ffdc6c;
    border-radius: 24rpx;
}
.got_egg .light {
    position: absolute;
    top: 0;
    width: 286rpx;
    height: 310rpx;
    left: 50%;
    transform: translateX(-50%);
}
.got_egg .title {
    margin-top: 40rpx;
    text-align: center;
    height: 84rpx;
    font-size: 60rpx;
    line-height: 84rpx;
    color: #fff;
}
.got_egg .egg_type {
    position: relative;
    margin-top: 20rpx;
    text-align: center;
}
.got_egg .egg_type .egg_pic {
    width: 158rpx;
    height: 200rpx;
}
.got_egg .egg_type .egg_coin {
    position: absolute;
    bottom: 50rpx;
    right: 200rpx;
    width: 72rpx;
    height: 76rpx;
}
.got_egg .egg_type .egg_text {
    text-align: center;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 36rpx;
color: #FFFFFF;
}
